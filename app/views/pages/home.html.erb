<div class="jumbotron">
	<h2 class="center">Welcome to Pinteresting!</h2>
	<div class="center">
	<% if user_signed_in? %>
		<%= link_to "Log Out", destroy_user_session_path, method: :delete, class: "btn btn-primary btn-lg" %>
	<% else %>	
		<%= link_to "Sign Up", new_user_registration_path, class: "btn btn-primary btn-lg" %>
